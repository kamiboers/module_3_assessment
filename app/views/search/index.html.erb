<h1>Search Results:</h1>
<h3><%= %></h3>
<% @stores.each do |store| %>

<div class="container">
	<div class="store-<%= store.phone %> ">
	<h3>Store Name: <%= store.name %></h3>
	City: <%= store.city %><br>
	Store Type: <%= store.type %><br>
	Distance: <%= store.distance %> miles<br>
	Phone #: <%= store.phone %><br>
</div>
</div>

<% end %>

